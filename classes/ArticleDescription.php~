<?php
/**
* ArticleDescription class - structure of article parameters: id, date, title 
*
* @version 1.0
*/
class ArticleDescription
{
	/**
	* @var int $id article identificator
	*/	
	public $id;
	
	/**
	* @var string $date article creation date
	*/	
	public $date;
	
	/**
	* @var string $title article title
	*/
	public $title;
}
?>